{
  "name": "MediaWikiBootstrap",
  "namemsg": "skinname-mediawikibootstrap",
  "descriptionmsg": "mediawikibootstrap-skin-desc",
  "version": "2.0.0",
  "url": "https://www.mediawiki.org/wiki/Skin:MediaWikiBootstrap",
  "author": [
    "[https://mediawiki.org/wiki/User:nasirkhan Nasir Khan Saikat]",
    "[https://github.com/nasirkhan/mediawiki-bootstrap/graphs/contributors Contributors]"
  ],
  "type": "skin",
  "requires": {
    "MediaWiki": ">= 1.40.0"
  },
  "ValidSkinNames": {
    "mediawikibootstrap": {
      "class": "SkinMediaWikiBootstrap",
      "args": [
        {
          "name": "mediawikibootstrap",
          "responsive": true,
          "bodyOnly": true,
          "styles": ["skins.mediawikibootstrap"],
          "scripts": ["skins.mediawikibootstrap.js"]
        }
      ]
    }
  },
  "MessagesDirs": {
    "MediaWikiBootstrap": ["i18n"]
  },
  "ResourceModules": {
    "skins.mediawikibootstrap": {
      "styles": {
        "./resources/bootstrap/css/bootstrap.min.css": { "media": "screen" },
        "./resources/css/style.css": { "media": "screen" }
      }
    },
    "skins.mediawikibootstrap.js": {
      "scripts": ["./resources/bootstrap/js/bootstrap.bundle.js"]
    }
  },
  "ResourceFileModulePaths": {
    "localBasePath": "",
    "remoteSkinPath": "MediaWikiBootstrap"
  },
  "AutoloadClasses": {
    "SkinMediaWikiBootstrap": "includes/MediaWikiBootstrap.skin.php"
  },
  "ResourceModuleSkinStyles": {},
  "manifest_version": 2
}
