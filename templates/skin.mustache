<div id="main" class="container p-4">

	<div class="row justify-content-sm-center">
		<div class="col-12 col-sm-10">
			<header class="mw-header">
				{{>Logo}}
				<nav class="navbar navbar-expand-lg bg-body-tertiary my-2">
					<div class="container-fluid">
						<a class="navbar-brand visible-xs" href="/">{{{html-title}}}</a>
						<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
						</button>
						<div class="collapse navbar-collapse" id="navbarSupportedContent">
						<ul class="navbar-nav me-auto mb-2 mb-lg-0">
							<li class="nav-item">
								<a class="nav-link active" aria-current="page" href="#">Home</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" href="#">Link</a>
							</li>
							
						</ul>
						{{{html-search-box}}}
						</div>
					</div>
				</nav>
			</header>
		</div>
	</div>

	<main id="content" class="mw-body">
		<div class="row justify-content-sm-center">
			<div class="col-12 col-sm-10">
				<div class="row">
					<div class="col content__header">
						{{>ContentIndicators}}
						{{>ContentHeading}}
						{{>ContentTagline}}
						{{>ContentNamespaces}}
						{{>ContentActions}}
					</div>
				</div>
				{{>ContentBody}}
				{{#data-toc}}{{>TableOfContents}}{{/data-toc}}
			</div>
		</div>
	</main>

	<div>	
		{{>Notifications}}
		{{>Dropdown}}{{>PersonalMenu}}
		{{>Sidebar}}
		{{>Notices}}
		{{>Languages}}
	</div>

</div>
<div id="footer" class="container my-4">
	{{>Footer}}
	{{{html-credit-line}}}
</div>